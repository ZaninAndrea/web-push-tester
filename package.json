{
    "scripts": {
        "start": "concurrently --kill-others \"watchify index.js -o bundle.js\" \"npm run serve\" \"fast-live-reload -p 4001 -sp 4000\"",
        "build": "browserify index.js -o bundle.js",
        "serve": "serve -l 4001"
    },
    "dependencies": {
        "serve": "^11.1.0",
        "web-push": "^3.3.5"
    },
    "devDependencies": {
        "browserify": "^16.5.0",
        "concurrently": "^4.1.2",
        "fast-live-reload": "^2.10.0",
        "watchify": "^3.11.1"
    }
}
